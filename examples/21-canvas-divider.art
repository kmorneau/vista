import "vista.art"!

ARTURO [
    Title: "Canvas Divider"
    Logo: "arturo-vista.png"
    Width: 900
    Height: 560
]

view [
    text "Above"
    divider
    canvas .id:"demo-canvas" .width: 160 .height: 80 [
        fill-pen "#f0f4ff"
        box [0 0] [160 80]
        pen "#2f5cff"
        line-width 3
        line [10 10] [150 70]
        pen "#1b1b1b"
        font "12px 'IBM Plex Sans'"
        text [10 24] "Canvas OK"
    ]
    text "Below"
]
