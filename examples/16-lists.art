import "vista.art"!

ARTURO [
    Title: "Lists"
    Logo: "arturo-vista.png"
    Width: 900
    Height: 560
]

choice: "Two"
picks: ["Red"]

view [
    title "Lists"
    text "Single select:"
    list choice ["One" "Two" "Three"]
    text "Selected:"
    text choice
    return
    text "Multi select:"
    list picks .multi:true ["Red" "Green" "Blue"]
    text "Selected:"
    text picks
    return
    text "Drop-list:"
    drop-list choice ["One" "Two" "Three"]
]
